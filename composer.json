{
    "name": "lson/auth",
    "description": "An authorization library in laravel",
    "type": "library",
    "keywords": ["laravel", "authorization"],
    "license": "MIT",
    "authors": [
        {
            "name": "lsrong",
            "email": "lsrong0414@gmail.com.cn"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "^5.5|^6.0|^7.0",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lson\\Authorization\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lson\\Authorization\\AuthorizationServiceProvider"
            ]
        }
    }
}
